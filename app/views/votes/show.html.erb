<div class="page-header">
  <%= link_to votes_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_vote_path(@vote), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show vote</h1>
</div>

<dl class="dl-horizontal">
  <dt>User:</dt>
  <dd><%= @vote.user_id %></dd>

  <dt>Quote:</dt>
  <dd><%= @vote.quote_id %></dd>

  <dt>Value:</dt>
  <dd><%= @vote.value %></dd>

</dl>
